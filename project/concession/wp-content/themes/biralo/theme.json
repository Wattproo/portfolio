				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "18px"
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "underline"
						}
					}
				}
			},
			"core/query-pagination-numbers": {
				"typography": {
					"letterSpacing": "5px"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"fontWeight": "500"
						}
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "#ffffff"
							}
						},
						"color": {
							"text": "#ffffff"
						}
					}
				},
				"typography": {
					"fontSize": "4.8em",
					"letterSpacing": "5px",
					"fontFamily": "var(--wp--preset--font-family--kanit)",
					"fontWeight": "600"
				}
			}
		},
		"color": {
			"background": "#ffffff",
			"text": "#333333"
		},
		"elements": {
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "3.5em"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.5em"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.3em"
				}
			},
			"link": {
